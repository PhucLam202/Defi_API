# Railway Production Environment Variables
NODE_ENV=production
PORT=3000

# API Configuration
API_BASE_URL=https://your-app-name.up.railway.app
API_VERSION=v1

# CORS Configuration
CORS_ORIGIN=https://your-app-name.up.railway.app

# Security
DISABLE_X_POWERED_BY=true

# Logging
LOG_LEVEL=info

# Health Check
HEALTH_CHECK_PATH=/

# Documentation
DOCS_PATH=/docs
OPENAPI_PATH=/openapi.json